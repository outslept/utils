{
  "name": "@outslept/utils",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.15.3",
  "description": "Collection of common JavaScript / TypeScript utils",
  "license": "MIT",
  "funding": "https://github.com/sponsors/outslept",
  "repository": {
    "type": "git",
    "url": "https://github.com/outslept/outslept.git"
  },
  "bugs": {
    "url": "https://github.com/outslept/outslept/issues"
  },
  "keywords": [
    "utils"
  ],
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.0",
    "@types/node": "^22.13.2",
    "@types/throttle-debounce": "^5.0.2",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.20.1",
    "eslint-plugin-format": "^1.0.1",
    "esno": "^4.8.0",
    "nano-staged": "^0.8.0",
    "simple-git-hooks": "^2.11.1",
    "throttle-debounce": "^5.0.2",
    "tsup": "^8.4.0",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm nano-staged"
  },
  "nano-staged": {
    "*.{js,ts,tsx,vue,md}": "eslint --fix"
  }
}
